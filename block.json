{
	"apiVersion": "1",
	"name": "sdb/shape",
	"title": "Shape Divider",
	"description": "Shape Divider is a web design tool that allows users to create unique and visually appealing dividers by customizing shapes and patterns,",
	"category": "widgets",
	"keywords": [
		"sdb",
		"sdb-shape-divider",
		"shap",
		"divider"
	],
	"textdomain": "shape-divider",
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"cId": {
			"type": "string",
			"default": ""
		},
		"shape": {
			"type": "string",
			"default": "curve"
		},
		"isInSection": {
			"type": "boolean",
			"default": false
		},
		"possition": {
			"type": "string",
			"default": "bottom"
		},
		"height": {
			"type": "object",
			"default": {
				"desktop": "150px",
				"tablet": "150px",
				"mobile": "150px"
			}
		},
		"width": {
			"type": "number",
			"default": 100
		},
		"color": {
			"type": "string",
			"default": "#fe6601"
		},
		"zindex":{
			"type":"number",
			"default": 0
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true,
			"columns": {
				"desktop": 1,
				"tablet": 1,
				"mobile": 1
			}
		}
	},
	"editorScript": "file:dist/editor.js",
	"script": "file:dist/script.js"
}