{
	"apiVersion": "1",
	"name": "sdb/shape",
	"title": "Shape Divider",
	"description": "Shape Divider is a web design tool that allows users to create unique and visually appealing dividers by customizing shapes and patterns,",
	"category": "widgets",
	"keywords": [
		"sdb",
		"sdb-shape-divider",
		"shape",
		"divider"
	],
	"textdomain": "shape-divider",
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"cId": {
			"type": "string",
			"default": ""
		},
		"background": {
			"type": "string",
			"default": {
				"type": "gradient",
				"gradient": "linear-gradient(0deg, rgba(32,182,249,1) 0%, rgba(20,255,237,1) 100%)"
			}
		},
		"padding": {
			"type": "object",
			"default": {
				"top": "300px",
				"right": "0px",
				"bottom": "150px",
				"left": "0px"
			}
		},
		"height": {
			"type": "object",
			"default": {
				"desktop": "100vh",
				"tablet": "100vh",
				"mobile": "100vh"
			}
		},
		"shape": {
			"type": "string",
			"default": "waves-opacity"
		},
		"shapePossition": {
			"type": "string",
			"default": "bottom"
		},
		"shapeHeight": {
			"type": "object",
			"default": {
				"desktop": "150px",
				"tablet": "150px",
				"mobile": "150px"
			}
		},
		"shapeWidth": {
			"type": "number",
			"default": 100
		},
		"shapeColor": {
			"type": "string",
			"default": "#fff"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true,
			"columns": {
				"desktop": 1,
				"tablet": 1,
				"mobile": 1
			}
		}
	},
	"editorScript": "file:dist/editor.js",
	"script": "file:dist/script.js"
}